{
  "name": "notion",
  "private": true,
  "description": "Fast and accurate React renderer for Notion. TS batteries included.",
  "repository": "3bases/notion",
  "author": "Alan Jo <sungle3737@gmail.com>",
  "license": "MIT",
  "version": "1.0.0",
  "engines": {
    "node": ">=12"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "link": "pnpm exec \"pnpm link\" --no-private",
    "dev": "turbo watch",
    "prebuild": "turbo clean",
    "prewatch": "turbo clean",
    "clean": "rm -rf packages/*/build packages/*/.turbo node_modules/.cache",
    "pretest": "turbo build",
    "test": "vitest",
    "coverage": "vitest --run --coverage",
    "lint": "eslint '**/*.{ts,tsx}' --fix",
    "format": "prettier '**/*.{js,jsx,ts,tsx}' --write",
    "predeploy": "turbo build",
    "deploy": "pnpm publish",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.1",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "tsup": "^6.2.3",
    "tsx": "^3.12.7",
    "turbo": "^1.9.3",
    "typescript": "^4.8.4",
    "vite": "^4.3.3",
    "vitest": "^0.30.1"
  }
}
