    "katex": "^0.15.3",
    "@3bases/notion-types": "workspace:^",
    "@3bases/notion-utils": "workspace:^",
    "prismjs": "^1.27.0",
    "react-fast-compare": "^3.2.0",
    "react-hotkeys-hook": "^3.0.3",
    "react-image": "^4.0.3",
    "react-modal": "^3.16.1",
    "react-pdf": "^5.7.1",
    "react-use": "^17.3.1"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^16.11.2",
    "@types/react": "^17.0.31",
    "clipboard-copy": "^4.0.1",
    "date-fns": "^2.28.0",
    "format-number": "^3.0.0",
    "lodash.throttle": "^4.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  }
