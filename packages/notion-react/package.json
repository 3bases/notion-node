{
  "name": "@3bases/notion-react",
  "version": "1.1.0-alpha.1",
  "type": "module",
  "description": "Fast and accurate React renderer for Notion.",
  "repository": "3bases/notion",
  "author": "Alan Jo <sungle3737@gmail.com>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "sideEffects": false,
  "files": [
    "build",
    "src/styles.css"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc && tsup",
